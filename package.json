{
  "name": "charmingsong-cli",
  "version": "4.0.7",
  "description": "Tools for quickly building projects",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "eslint": "eslint src --ext .ts",
    "prepare": "npm run eslint && npm run build",
    "npm:publish": "npm publish --registry  \"https://registry.npmjs.org/\""
  },
  "bin": {
    "cs": "bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-songsong/charmingsong-cli.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint"
    }
  },
  "keywords": [
    "charmingsong",
    "cli",
    "node",
    "typescript"
  ],
  "author": "huxuesong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/web-songsong/charmingsong-cli/issues"
  },
  "homepage": "https://github.com/web-songsong/charmingsong-cli#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^4.0.0",
    "download-git-repo": "^3.0.2",
    "handlebars": "^4.5.1",
    "inquirer": "^7.0.0",
    "metalsmith": "^2.3.0",
    "ora": "^4.0.2",
    "rimraf": "^3.0.0",
    "user-home": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.9",
    "typescript": "^3.6.4"
  }
}
